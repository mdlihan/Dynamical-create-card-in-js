var container=document.querySelector('.container')
const create=()=>{
   cards.map((ele)=>{
     console.log(ele)
     const newdiv=document.createElement('div')
      newdiv.classList.add('card')
      newdiv.innerHTML=`<h4>${ele.titel}</h4>
                        <p>${ele.body}</p>`
      container.appendChild(newdiv)                  
   })
}
create();